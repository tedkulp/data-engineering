<h1>Uploads</h1>

<table class="table">
  <thead>
    <tr>
      <th># Records</th>
      <th>Uploaded at</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= upload.num_records %></td>
        <td><%= upload.created_at %></td>
        <td><%= link_to 'Show', upload %></td>
        <td><%= link_to 'Destroy', upload, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'Upload New Data', new_upload_path %>
